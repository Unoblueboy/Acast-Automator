{
    "name": "Acast Enhancement Suite",
    "description": "Enhances your experience on the Acast website",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "tabs",
      "https://www.acast.com/*"
    ],
    "background": {
      "scripts": ["jquery-3.3.1.js", "background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Acast Enhancement Suite",
      "default_icon":"icon.png"
    },
    "icons": { "196": "icon.png" },
    "manifest_version": 2
  }